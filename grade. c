#include<stdio.h>
void main()
{
int marks;
printf("enter your marks");
scanf("%d",&marks);
if(marks<0 || marks>100)
{
printf("\nwrong input");
}
if(marks<=50)
{
printf("grade F");
}
else if(marks<=60)
{
printf("grade E");
}
else if(marks<=70)
{
printf("grade D");
}
else if(marks<=80)
{
printf("grade C");
}
else if(marks<=90)
{
printf("grade B");
}
else
{
printf("grade A");
}
}
